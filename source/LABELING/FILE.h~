#ifndef __FILE_H__
#define __FILE_H__

#include <stdio.h>
#include <string.h>

#define FILE_WRITE 1
#define FILE_READ 0

extern void OriginalFileName(int fnum, char* fname);
extern void ResultFileName(int fnum, char* fname);
extern FILE* OriginalFileDescriptor(int fnum, int mode);
extern FILE* ResultFileDescriptor(int fnum, int mode);

#endif
